{
  "author": "Chris Shaw <Chris.Shaw@merkleinc.com.au>",
  "description": "Generates the CSS and JavaScript for visually appealing experiences in AEM.",
  "engines": {
    "node": ">= 16",
    "yarn": ">= 1.10.x < 2"
  },
  "private": true,
  "name": "ui.frontend",
  "version": "1.0.0-SNAPSHOT",
  "scripts": {
    "aem:build:dev": "cross-env NODE_ENV=development vite build --mode development",
    "aem:build": "cross-env NODE_ENV=production vite build --mode production",
    "bootstrap": "ts-node --project ./support/tsconfig.json support/generators/bootstrap.ts",
    "build-storybook": "build-storybook",
    "lint:fix": "eslint --fix --ext .css,.ts, src",
    "lint": "eslint --ext .css,.ts, src",
    "preaem:build:dev": "yarn bootstrap",
    "preaem:build": "yarn bootstrap",
    "preserve": "yarn bootstrap",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "serve:hino": "yarn serve --config ./support/configs/vite/hino.config.ts",
    "serve:lexus": "yarn serve --config ./support/configs/vite/lexus.config.ts",
    "serve:mazda": "yarn serve --config ./support/configs/vite/mazda.config.ts",
    "serve:power-alliance": "yarn serve --config ./support/configs/vite/power-alliance.config.ts",
    "serve:powertorque": "yarn serve --config ./support/configs/vite/powertorque.config.ts",
    "serve:suzuki": "yarn serve --config ./support/configs/vite/suzuki.config.ts",
    "serve:toyota": "yarn serve --config ./support/configs/vite/toyota.config.ts",
    "serve": "cross-env DEV_SERVER=true vite",
    "storybook": "start-storybook -p 6006",
    "test:coverage": "jest --coverage",
    "test": "jest"
  },
  "workspaces": {
    "nohoist": [
      "@fortawesome/fontawesome-free",
      "@fortawesome/fontawesome-svg-core",
      "@fortawesome/free-brands-svg-icons",
      "@fortawesome/free-solid-svg-icons",
      "lodash"
    ]
  },
  "dependencies": {
    "@forgerock/javascript-sdk": "^3.3.1",
    "@vitejs/plugin-react": "^1.3.2",
    "axios": "^1.1.3",
    "formik": "^2.2.9",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "react-datepicker": "^4.8.0",
    "react-functional-select": "3.3.4",
    "react-google-recaptcha": "3.0.0-alpha.1",
    "react-select": "^5.7.0",
    "react-table": "^7.8.0",
    "react-tabs": "^5.1.0",
    "rollup-plugin-typescript-paths": "^1.3.1",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@adobe/aem-site-theme-builder": "^5.2.1",
    "@aem-vite/vite-aem-plugin": "2.2.1",
    "@nabla/vite-plugin-eslint": "^1.4.0",
    "@storybook/addon-a11y": "6.5.9",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addon-queryparams": "^6.2.9",
    "@storybook/builder-vite": "^0.1.36",
    "@storybook/node-logger": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "0.0.11",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/react-table": "^7.7.12",
    "@types/styled-components": "^5.1.25",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^27.5.1",
    "chalk": "^4.1.2",
    "child_process": "^1.0.2",
    "cli-table3": "^0.6.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "fast-glob": "^3.2.11",
    "js-yaml": "^4.1.0",
    "jsdom": "^19.0.0",
    "postcss": "^8.4.6",
    "postcss-import": "^14.0.2",
    "postcss-pxtorem": "^6.0.0",
    "postcss-reporter": "^7.0.5",
    "postcss-sorting": "^7.0.1",
    "prettier": "^2.7.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-otp-input": "^2.4.0",
    "react-table": "^7.8.0",
    "react-window": "^1.8.7",
    "replace-in-file": "^6.3.2",
    "seamless-scroll-polyfill": "^2.2.0",
    "styled-components": "^4.1.3",
    "styled-normalize": "^8.0.7",
    "stylelint": "^14.5.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "ts-node": "^10.5.0",
    "typescript": "^4.6.4",
    "use-debounce": "^8.0.1",
    "vite": "2.8.1",
    "vite-plugin-eslint": "^1.6.1",
    "vite-tsconfig-paths": "^3.4.0",
    "web-vitals": "^2.1.4",
    "zustand": "^4.0.0-rc.1"
  }
}
