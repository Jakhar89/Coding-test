<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:redirectTarget="/content/sscp/mazda/au"
        cq:template="/conf/sscp/settings/wcm/templates/sscp-content-page"
        jcr:mixinTypes="[cq:LiveRelationship,cq:LiveSync]"
        jcr:primaryType="cq:PageContent"
        jcr:title="Mazda"
        sling:resourceType="sscp/components/page"
        brandSlug="mazda"
        brandSlug_override="true"
        pwaCachestrategy="staleWhileRevalidate"
        pwaDisplay="standalone"
        pwaOrientation="any">
        <root
            jcr:mixinTypes="[cq:LiveRelationship]"
            jcr:primaryType="nt:unstructured"
            sling:resourceType="sscp/components/container"
            layout="responsiveGrid">
            <container
                jcr:mixinTypes="[cq:LiveRelationship]"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sscp/components/container"
                layout="responsiveGrid"/>
        </root>
        <cq:LiveSyncConfig
            cq:isDeep="{Boolean}true"
            cq:master="/content/sscp/blueprint"
            cq:rolloutConfigs="[/libs/msm/wcm/rolloutconfigs/default]"
            jcr:primaryType="cq:LiveCopy"/>
    </jcr:content>
    <au/>
</jcr:root>
