<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[]"
        cq:template="/conf/sscp/settings/wcm/templates/xf-web-variation"
        cq:xfMasterVariation="{Boolean}true"
        cq:xfVariantType="web"
        jcr:primaryType="cq:PageContent"
        jcr:title="Header"
        sling:resourceType="sscp/components/xfpage">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="sscp/components/container"
            id="site-header"
            layout="simple">
            <header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sscp/components/header"
                isLogoReversed="true"
                logoAltText="alt text"
                logoPath="/content/dam/sscp/jcr:content"
                logoRedirectUrl="/content/sscp/powertorque/au/en/homepage/accounts">
                <menuItem jcr:primaryType="nt:unstructured">
                    <item0
                        jcr:primaryType="nt:unstructured"
                        flag="false"
                        icon="login-reversed"
                        text="Log in"
                        url="/content/sscp/powertorque/au/en/homepage/login"/>
                    <item1
                        jcr:primaryType="nt:unstructured"
                        flag="true"
                        text="Accounts"
                        url="/content/sscp/powertorque/au/en/homepage/accounts"/>
                    <item2
                        jcr:primaryType="nt:unstructured"
                        flag="true"
                        text="Repayments"
                        url="/content/sscp/powertorque/au/en/homepage/repayments"/>
                    <item3
                        jcr:primaryType="nt:unstructured"
                        flag="true"
                        icon="profile-reversed"
                        text="Profile"
                        url="/content/sscp/powertorque/au/en/homepage/profile"/>
                    <item4
                        jcr:primaryType="nt:unstructured"
                        flag="true"
                        icon="logout-reversed"
                        text="Log out"
                        url="/content/sscp/powertorque/au/en/homepage/logout"/>
                </menuItem>
            </header>
            <notificationbanner
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sscp/components/notificationbanner"
                isNotificationHasNoLimit="false"
                notificationDescription="Welcome to the fresh new customer portal. We're exciting to be adding more functionality soon, keep an eye out for news!"
                notificationId="generalNotification"
                notificationType="info"/>
            <notificationbanner_1610797233
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sscp/components/notificationbanner"
                isNotificationHasNoLimit="false"
                notificationDescription="You have been inactive for a few minutes. For your security &amp; privacy you will be automatically logged out after 5 minutes."
                notificationId="idleWarningNotification"
                notificationType="warning"/>
        </root>
    </jcr:content>
</jcr:root>
