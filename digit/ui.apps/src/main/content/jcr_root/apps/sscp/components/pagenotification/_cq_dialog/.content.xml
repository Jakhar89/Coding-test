<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Page Notification Properties"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <customTab
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Custom"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                    <isMaintainencePage
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                        name="./isMaintainencePage"
                        text="Is Maintainence Page (Please check if it's maintenance page)"
                        uncheckedValue="false"
                        value="true"/>
                    <icon
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Icon Class Name"
                        name="./icon"
                        required="{Boolean}true"/>
                    <title
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Title"
                        name="./title"
                        required="{Boolean}true"/>
                    <description
                        jcr:primaryType="nt:unstructured"
                        cq:showOnCreate="{Boolean}true"
                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                        useFixedInlineToolbar="{Boolean}true"
                        fieldLabel="Description"
                        required="{Boolean}true"
                        name="./description">
                        <rtePlugins
                            jcr:primaryType="nt:unstructured"
                            sling:resourceSuperType="/apps/sscp/components/rteplugin-config/rtePlugins"/>
                        <uiSettings
                            jcr:primaryType="nt:unstructured"
                            sling:resourceSuperType="/apps/sscp/components/rteplugin-config/uiSettings"/>
                    </description>
                    <mobileDescription
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Mobile Description (Required only for auto logout page)"
                        name="./mobileDescription"/>
                    <buttonText
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Button Text"
                        name="./buttonText"/>
                    <buttonUrl
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                        fieldLabel="Button URL"
                        name="./buttonUrl"
                        rootPath="/content/sscp/"/>
                                        </items>
                                    </column1>
                                </items>
                            </columns>
                        </items>
                    </customTab>
                    <analyticsTab
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Analytics"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <analyticsTrackEventName
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="accountUnlock"
                                                fieldLabel="Event Tracking ID"
                                                name="./analyticsTrackEventName"
                                                value="accountUnlock"/>
                                        </items>
                                    </column1>
                                </items>
                            </columns>
                        </items>
                    </analyticsTab>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
