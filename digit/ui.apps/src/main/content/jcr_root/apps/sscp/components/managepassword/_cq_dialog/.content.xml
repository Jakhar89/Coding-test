<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Manage Password Properties"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/tabs"
                    maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <sectionTab
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Section Details"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <managePasswordTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Manage Password"
                                                fieldLabel="Section Title"
                                                name="./managePasswordTitle"
                                                required="{Boolean}true"
                                                value="Manage Password"/>
                                            <passwordText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Password"
                                                fieldLabel="Password Label"
                                                name="./passwordText"
                                                required="{Boolean}true"
                                                value="Password"/>
                                            <saveButtonText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="save changes"
                                                fieldLabel="Save button text"
                                                name="./saveButtonText"
                                                required="{Boolean}true"
                                                value="save changes"/>
                                            <cancelButtonText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="cancel"
                                                fieldLabel="Cancel button text"
                                                name="./cancelButtonText"
                                                required="{Boolean}true"
                                                value="cancel"/>
                                        </items>
                                    </column1>
                                </items>
                            </columns>
                        </items>
                    </sectionTab>
                    <step1
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Manage Password Step 1"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/sscp/components/communicationsconfig/cq:dialog/content/items/tabs/items/managePassword/items/columns"/>
                        </items>
                    </step1>
                    <step2
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Verify Mobile Step 2"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/sscp/components/communicationsconfig/cq:dialog/content/items/tabs/items/verifyMobile/items/columns"/>
                        </items>
                    </step2>
                    <step3
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Thank you Step 3"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/sscp/components/communicationsconfig/cq:dialog/content/items/tabs/items/thankyou/items/columns"/>
                        </items>
                    </step3>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
