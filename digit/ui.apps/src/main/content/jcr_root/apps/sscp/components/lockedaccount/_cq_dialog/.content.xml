<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Locked Account Properties"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/tabs"
                    maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <step1
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Step 1"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <lockedIcon
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="lock"
                                                fieldLabel="Locked account Icon Class Name"
                                                name="./lockedIcon"
                                                required="{Boolean}true"
                                                value="lock"/>
                                            <lockedTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Your account has been locked"
                                                fieldLabel="Locked account Title"
                                                name="./lockedTitle"
                                                required="{Boolean}true"
                                                value="Your account has been locked"/>
                                            <lockedDescription
                                                jcr:primaryType="nt:unstructured"
                                                cq:showOnCreate="{Boolean}true"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                useFixedInlineToolbar="{Boolean}true"
                                                fieldLabel="Locked account description"
                                                required="{Boolean}true"
                                                name="./lockedDescription">
                                                <rtePlugins
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/sscp/components/rteplugin-config/rtePlugins"/>
                                                <uiSettings
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/sscp/components/rteplugin-config/uiSettings"/>
                                            </lockedDescription>
                                            <lockedButtonText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="unlock account"
                                                fieldLabel="Unlock Button Text"
                                                name="./lockedButtonText"
                                                required="{Boolean}true"
                                                value="unlock account"/>
                                            <eventName
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="accountUnlock"
                                                fieldLabel="Event Tracking ID"
                                                name="./eventName"
                                                value="accountUnlock"/>
                                        </items>
                                    </column1>
                                </items>
                            </columns>
                        </items>
                    </step1>
                    <step2
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Step 2"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/sscp/components/communicationsconfig/cq:dialog/content/items/tabs/items/verifyEmail/items/columns"/>
                        </items>
                    </step2>
                    <step3
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Step 3"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/sscp/components/communicationsconfig/cq:dialog/content/items/tabs/items/verifyMobile/items/columns"/>
                        </items>
                    </step3>
                    <step4
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Step 4"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <unlockedIcon
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="unlocked"
                                                fieldLabel="Unlocked account Icon Class Name"
                                                name="./unlockedIcon"
                                                required="{Boolean}true"
                                                value="unlocked"/>
                                            <unlockedTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Your account has been unlocked"
                                                fieldLabel="Unlocked account Title"
                                                name="./unlockedTitle"
                                                required="{Boolean}true"
                                                value="Your account has been unlocked"/>
                                            <unlockedDescription
                                                jcr:primaryType="nt:unstructured"
                                                cq:showOnCreate="{Boolean}true"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                useFixedInlineToolbar="{Boolean}true"
                                                fieldLabel="Unlocked account Description"
                                                required="{Boolean}true"
                                                name="./unlockedDescription">
                                                <rtePlugins
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/sscp/components/rteplugin-config/rtePlugins"/>
                                                <uiSettings
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/sscp/components/rteplugin-config/uiSettings"/>
                                            </unlockedDescription>
                                            <loginButtonText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Login &amp; Continue"
                                                fieldLabel="Login Button Text"
                                                name="./loginButtonText"
                                                required="{Boolean}true"
                                                value="Login &amp; Continue"/>
                                            <resetButtonText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="reset password"
                                                fieldLabel="Reset Button Text"
                                                name="./resetButtonText"
                                                required="{Boolean}true"
                                                value="reset password"/>
                                        </items>
                                    </column1>
                                </items>
                            </columns>
                        </items>
                    </step4>
                    <step5
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Step 5"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <columns
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/include"
                                                path="/apps/sscp/components/communicationsconfig/cq:dialog/content/items/tabs/items/managePassword/items/columns"/>
                                            <setPasswordText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Set Password &amp; log in"
                                                fieldLabel="Set password button text"
                                                name="./setPasswordText"
                                                required="{Boolean}true"
                                                value="Set Password &amp; log in"/>
                                        </items>
                                    </column1>
                                </items>
                            </columns>
                        </items>
                    </step5>
                    <step6
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Step 6"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/sscp/components/communicationsconfig/cq:dialog/content/items/tabs/items/thankyou/items/columns"/>
                        </items>
                    </step6>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
