<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Brand Variables Config"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <columns
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column1
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <brandName
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                emptyText="Enter the Brand name"
                                fieldLabel="Enter the Brand name"
                                name="./brandName"
                                value="Enter the Brand name"/>
                            <phoneNumberOne
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                emptyText="Enter the first Phone Number"
                                fieldLabel="Enter the first Phone Number Text"
                                name="./brandPhNumber1"
                                value="Enter the first Phone number"/>
                            <phoneNumberTwo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                emptyText="Enter the second Phone Number"
                                fieldLabel="Enter the second Phone Number"
                                name="./brandPhNumber2"
                                value="Enter the second Phone Number"/>
                            <emailOne
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                emptyText="Enter the first Email"
                                fieldLabel="Enter the first Email"
                                name="./brandEmail1"
                                value="Enter the first Email"/>
                            <emailTwo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                emptyText="Enter the second Email"
                                fieldLabel="Enter the second Email"
                                name="./brandEmail2"
                                value="Enter the second Email"/>
                            <postalAdd
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                emptyText="Enter the Postal Address"
                                fieldLabel="Enter the Postal Address"
                                name="./brandPostalAddress"
                                value="Enter the Postal Address"/>
                            <transactionHistoryFooter
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                emptyText="Enter the Transaction History Footer Text"
                                fieldLabel="Enter the Transaction History Footer Text"
                                name="./brandTransactionHistoryFooter"
                                value="Enter the Transaction History Footer Text"/>
                        </items>
                    </column1>
                </items>
            </columns>
        </items>
    </content>
</jcr:root>
