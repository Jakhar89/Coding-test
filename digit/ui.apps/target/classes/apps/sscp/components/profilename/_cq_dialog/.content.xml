<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Profile Name Properties"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <profileNameTitle
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        emptyText="Your name"
                        fieldLabel="Section Title"
                        name="./profileNameTitle"
                        required="{Boolean}true"
                        value="Your name"/>
                    <title
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        emptyText="Title"
                        fieldLabel="Profile Title Label"
                        name="./title"
                        required="{Boolean}true"
                        value="Title"/>
                    <givenName
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        emptyText="Given name"
                        fieldLabel="Given Name Label"
                        name="./givenName"
                        required="{Boolean}true"
                        value="Given name"/>
                    <middleName
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        emptyText="Middle name"
                        fieldLabel="Middle Name Label"
                        name="./middleName"
                        required="{Boolean}true"
                        value="Middle name"/>
                    <familyName
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        emptyText="Family name"
                        fieldLabel="Family Name Label"
                        name="./familyName"
                        required="{Boolean}true"
                        value="Family name"/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
