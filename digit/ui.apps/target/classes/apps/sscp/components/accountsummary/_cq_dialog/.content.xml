<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Account Summary Properties"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/layouts/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <accountSummaryDetail
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Account Summary Detail"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <accountNumberTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Account Number"
                                                fieldLabel="Account Number Title"
                                                name="./accountNumberTitle"
                                                required="{Boolean}true"
                                                value="Account Number"/>
                                            <paidToDateTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Paid To Date"
                                                fieldLabel="Paid To Date Title"
                                                name="./paidToDateTitle"
                                                required="{Boolean}true"
                                                value="Paid To Date"/>
                                            <outstandingBalanceTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Outstanding Balance"
                                                fieldLabel="Outstanding Balance Title"
                                                name="./outstandingBalanceTitle"
                                                required="{Boolean}true"
                                                value="Outstanding Balance "/>
                                            <loanProgressTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Loan Progress"
                                                fieldLabel="Loan Progress Title"
                                                name="./loanProgressTitle"
                                                required="{Boolean}true"
                                                value="Loan Progress"/>
                                            <nextRepaymentDueTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Next Repayment Due "
                                                fieldLabel="Next Repayment Due Title"
                                                name="./nextRepaymentDueTitle"
                                                required="{Boolean}true"
                                                value="Next Repayment Due"/>
                                            <paidToDateTooltip
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Paid To Date Tooltip"
                                                fieldLabel="Paid To Date Tooltip"
                                                name="./paidToDateTooltip"
                                                required="{Boolean}true"
                                                value="Paid To Date Tooltip"/>
                                            <outstandingBalanceTooltip
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Outstanding Balance Tooltip"
                                                fieldLabel="Outstanding Balance Tooltip"
                                                name="./outstandingBalanceTooltip"
                                                required="{Boolean}true"
                                                value="Outstanding Balance  Tooltip"/>
                                        </items>
                                    </column1>
                                </items>
                            </columns>
                        </items>
                    </accountSummaryDetail>
                    <recentTransactionsDetail
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Recent Transactions"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <actionMenuMulti
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="Action Menu List">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/container"
                                                    name="./actionMenuTypeItem">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <itemWell
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/well">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <actionMenu
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                            emptyText="Action Menu Item"
                                                                            fieldLabel="Enter the Menu Item Label"
                                                                            name="./actionMenu"
                                                                            required="{Boolean}true"
                                                                            value="Action Menu Item"/>
                                                                        <actionMenuLink
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                                            fieldLabel="Enter Action Menu link"
                                                                            name="./actionMenuLink"
                                                                            rootPath="/content/sscp/"/>
                                                                    </items>
                                                                </itemWell>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </actionMenuMulti>
                                            <recentTransactionsTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Recent Transactions"
                                                fieldLabel="Recent Transactions Title"
                                                name="./recentTransactionsTitle"
                                                required="{Boolean}true"
                                                value="Recent Transactions"/>
                                            <recentTransactionsTooltip
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Recent transactions are shown as of yesterday."
                                                fieldLabel="Recent Transactions Tooltip"
                                                name="./recentTransactionsTooltip"
                                                required="{Boolean}true"
                                                value="Recent transactions are shown as of yesterday."/>

                                            <recentTransactionsNoTransacText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="No transaction details"
                                                fieldLabel="No Transaction Details"
                                                name="./noTransactionsText"
                                                required="{Boolean}true"
                                                value="No transaction details"/>
                                            <buttonLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Button Label"
                                                fieldLabel="Button Label"
                                                name="./buttonLabel"
                                                required="{Boolean}true"
                                                value="Button Label"/>
                                            <buttonLink
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                fieldLabel="Provide Button link"
                                                name="./buttonLink"
                                                rootPath="/content/sscp/"/>
                                            <date
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Table Heading - Date"
                                                fieldLabel="Provide Table Heading - Date Label"
                                                name="./date"
                                                required="{Boolean}true"
                                                value="Date"/>
                                            <paymentAmount
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Table Heading - Payment Amount"
                                                fieldLabel="Provide Table Heading - Payment Amount Label"
                                                name="./paymentAmount"
                                                required="{Boolean}true"
                                                value="Payment Amount"/>
                                            <balance
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Table Heading - Balance"
                                                fieldLabel="Provide Table Heading - Balance Label"
                                                name="./balance"
                                                required="{Boolean}true"
                                                value="Balance"/>
                                        </items>
                                    </column1>
                                </items>
                            </columns>
                        </items>
                    </recentTransactionsDetail>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
