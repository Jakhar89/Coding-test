<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Login Properties"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/tabs"
                    maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <step1
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Step 1"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <loginTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Log in"
                                                fieldLabel="Login Title"
                                                name="./loginTitle"
                                                required="{Boolean}true"
                                                value="Log in"/>
                                            <loginDescription
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="To log in please use the email address and password you have nominated."
                                                fieldLabel="Login Description"
                                                name="./loginDescription"
                                                required="{Boolean}true"
                                                value="To log in please use the email address and password you have nominated."/>
                                            <forgotPasswordText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Forgot your password"
                                                fieldLabel="Forgot password text"
                                                name="./forgotPasswordText"
                                                required="{Boolean}true"
                                                value="Forgot your password"/>
                                            <forgotPasswordUrl
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                fieldLabel="Forgot password URL"
                                                name="./forgotPasswordUrl"
                                                required="{Boolean}true"
                                                rootPath="/content/sscp/"/>
                                            <loginButton
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Log in"
                                                fieldLabel="Login button text"
                                                name="./loginButton"
                                                required="{Boolean}true"
                                                value="Log in"/>
                                            <loginContact
                                                jcr:primaryType="nt:unstructured"
                                                cq:showOnCreate="{Boolean}true"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                useFixedInlineToolbar="{Boolean}true"
                                                fieldLabel="Login Contact details"
                                                name="./loginContact">
                                                <rtePlugins
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/sscp/components/rteplugin-config/rtePlugins"/>
                                                <uiSettings
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/sscp/components/rteplugin-config/uiSettings"/>
                                            </loginContact>
                                            <registerIcon
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Icon Class Name"
                                                name="./registerIcon"
                                                required="{Boolean}true"/>
                                            <registerTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Don't have an account set up?"
                                                fieldLabel="Register Title"
                                                name="./registerTitle"
                                                required="{Boolean}true"
                                                value="Don't have an account set up?"/>
                                            <registerButton
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Register here"
                                                fieldLabel="Register button text"
                                                name="./registerButton"
                                                required="{Boolean}true"
                                                value="Register here"/>
                                            <registerButtonUrl
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                fieldLabel="Register Button URL"
                                                name="./registerButtonUrl"
                                                required="{Boolean}true"
                                                rootPath="/content/sscp/"/>
                                        </items>
                                    </column1>
                                </items>
                            </columns>
                        </items>
                    </step1>
                    <step2
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Step 2"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/sscp/components/communicationsconfig/cq:dialog/content/items/tabs/items/verifyMobile/items/columns"/>
                        </items>
                    </step2>
                    <step3
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Step 3"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <termsTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Terms and conditions"
                                                fieldLabel="Terms Section Title"
                                                name="./termsTitle"
                                                required="{Boolean}true"
                                                value="Terms and conditions"/>
                                            <agreeTermsTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Agree to Terms and Conditions"
                                                fieldLabel="Agree terms title"
                                                name="./agreeTermsTitle"
                                                required="{Boolean}true"
                                                value="Agree to Terms and Conditions"/>
                                            <agreeTerms
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="I have read and accept all the Terms and Conditions"
                                                fieldLabel="Read terms text"
                                                name="./agreeTerms"
                                                required="{Boolean}true"
                                                value="I have read and accept all the Terms and Conditions"/>
                                            <agreeTermsButton
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Continue"
                                                fieldLabel="Agree terms button text"
                                                name="./agreeTermsButton"
                                                required="{Boolean}true"
                                                value="Continue"/>
                                            <termsOfUse
                                                jcr:primaryType="nt:unstructured"
                                                cq:showOnCreate="{Boolean}true"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                useFixedInlineToolbar="{Boolean}true"
                                                fieldLabel="Enter Terms of Use"
                                                name="./termsOfUse">
                                                <rtePlugins
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/sscp/components/rteplugin-config/rtePlugins"/>
                                                <uiSettings
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/sscp/components/rteplugin-config/uiSettings"/>
                                            </termsOfUse>
                                        </items>
                                    </column1>
                                </items>
                            </columns>
                        </items>
                    </step3>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
