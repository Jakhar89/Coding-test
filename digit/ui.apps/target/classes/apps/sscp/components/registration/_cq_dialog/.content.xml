<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Registration Properties"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/tabs"
                    maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <step1
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Step 1"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <registerTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Register your account"
                                                fieldLabel="Register Title"
                                                name="./registerTitle"
                                                required="{Boolean}true"
                                                value="Register your account"/>
                                            <registerDescription
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="To register please use an email address known to us or used during your finance application."
                                                fieldLabel="Register Description"
                                                name="./registerDescription"
                                                required="{Boolean}true"
                                                value="To register please use an email address known to us or used during your finance application."/>
                                            <registerButton
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Continue"
                                                fieldLabel="Register button text"
                                                name="./registerButton"
                                                required="{Boolean}true"
                                                value="Continue"/>
                                            <registerContact
                                                jcr:primaryType="nt:unstructured"
                                                cq:showOnCreate="{Boolean}true"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                useFixedInlineToolbar="{Boolean}true"
                                                fieldLabel="Register Contact details"
                                                name="./registerContact">
                                                <rtePlugins
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/sscp/components/rteplugin-config/rtePlugins"/>
                                                <uiSettings
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/sscp/components/rteplugin-config/uiSettings"/>
                                            </registerContact>
                                            <loginIcon
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Icon Class Name"
                                                name="./loginIcon"
                                                required="{Boolean}true"/>
                                            <loginTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Already have an account?"
                                                fieldLabel="Login Title"
                                                name="./loginTitle"
                                                required="{Boolean}true"
                                                value="Already have an account?"/>
                                            <loginButton
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Log In"
                                                fieldLabel="Login button text"
                                                name="./loginButton"
                                                required="{Boolean}true"
                                                value="Log In"/>
                                            <loginButtonUrl
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                fieldLabel="Login Button URL"
                                                name="./loginButtonUrl"
                                                required="{Boolean}true"
                                                rootPath="/content/sscp/"/>
                                        </items>
                                    </column1>
                                </items>
                            </columns>
                        </items>
                    </step1>
                    <step2
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Step 2"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <termsTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Terms and conditions"
                                                fieldLabel="Terms Section Title"
                                                name="./termsTitle"
                                                required="{Boolean}true"
                                                value="Terms and conditions"/>
                                            <agreeTermsTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Agree to Terms and Conditions"
                                                fieldLabel="Agree terms title"
                                                name="./agreeTermsTitle"
                                                required="{Boolean}true"
                                                value="Agree to Terms and Conditions"/>
                                            <agreeTerms
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="I have read and accept all the Terms and Conditions"
                                                fieldLabel="Read terms text"
                                                name="./agreeTerms"
                                                required="{Boolean}true"
                                                value="I have read and accept all the Terms and Conditions"/>
                                            <agreeTermsButton
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Continue"
                                                fieldLabel="Agree terms button text"
                                                name="./agreeTermsButton"
                                                required="{Boolean}true"
                                                value="Continue"/>
                                        </items>
                                    </column1>
                                </items>
                            </columns>
                        </items>
                    </step2>
                    <step3
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Step 3"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/sscp/components/communicationsconfig/cq:dialog/content/items/tabs/items/verifyEmail/items/columns"/>
                        </items>
                    </step3>
                    <step4
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Step 4"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/sscp/components/communicationsconfig/cq:dialog/content/items/tabs/items/verifyMobile/items/columns"/>
                        </items>
                    </step4>
                    <step5
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Step 5"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/sscp/components/communicationsconfig/cq:dialog/content/items/tabs/items/managePassword/items/columns"/>
                            <resetPasswordText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                emptyText="Reset Password"
                                fieldLabel="Reset password button text"
                                name="./resetPasswordText"
                                required="{Boolean}true"
                                value="Reset Password"/>
                        </items>
                    </step5>
                    <step6
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Step 6"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/sscp/components/communicationsconfig/cq:dialog/content/items/tabs/items/thankyou/items/columns"/>
                        </items>
                    </step6>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
